{
  "console_title_template": "{{if .Segments.Git.RepoName}} {{.Segments.Git.RepoName}} {{else}} {{.Folder}} {{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#20a9a9",
          "template": "\u256d\u2500"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#7b7b7b",
          "template": " üêç {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}({{ .Venv }}) {{ end }}{{ end }}"
        },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "background": "#1982c4",
          "foreground": "#100302",
          "template": "{{ if gt .Code 0 }}ü•µ{{ else }}üòé{{ end }} {{ .UserName }}@{{ .HostName }}"
        },
        {
          "type": "path",
          "style": "diamond",
          "background": "#ffca3a",
          "foreground": "#100302",
          "leading_diamond": "<transparent,background>\ue0b4</>",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "style": "agnoster_full"
          },
          "template": " üìÅ {{ .Path }} "
        },
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b4</>",
          "trailing_diamond": "\ue0b4",
          "background_templates": [
            "{{ if eq .Code 0 }}#8ac926{{ else }}#ff595e{{ end }}"
          ],
          "foreground": "#100302",
          "template": "{{ if eq .Code 0 }} üçÄ nice{{ else }} üî• fail{{ end }} ",
          "properties": {
            "always_enabled": true,
            "display_exit_code": false,
            "mapping": {
              "0": " clean "
            },
            "default_symbol": " error "
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#20a9a9",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffca3a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff595e{{ end }}",
            "{{ if gt .Ahead 0 }}#1982c4{{ end }}",
            "{{ if gt .Behind 0 }}#8ac926{{ end }}"
          ],
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} \u2502{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone": "\u2262",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 ",
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#20a9a9",
          "style": "plain",
          "template": "\u2570\u2500"
        },
        {
          "type": "text",
          "foreground": "#ffffff",
          "style": "plain",
          "template": ">"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}